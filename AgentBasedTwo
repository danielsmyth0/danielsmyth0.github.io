import random
agents = []

#variables
agents.append([random.randint(0,99),random.randint(0,99)])
agents.append([random.randint(0,99),random.randint(0,99)])
#y0 = random.randint(0,99)
#x0 = random.randint(0,99)
#y1 = random.randint(0,99)
#x1 = random.randint(0,99)


#move 1 step 1
if random.random() < 0.5:
    agents[0][0] += 1
else:
    agents[0][0] -= 1

if random.random() < 0.5:
    agents[0][1] += 1
else:
    agents[0][1] -= 1

    

#move 1 step 2
if random.random() < 0.5:
    agents[1][0] += 1
else:
    agents[1][0] -= 1

if random.random() < 0.5:
    agents[1][1] += 1
else:
    agents[1][1] -= 1

    
#distance between
answer = (((agents[0][0] - agents[1][0])**2) + ((agents[0][1] - agents[1][1])**2))**0.5
print(answer) 

print(agents[0][0], agents[0][1], agents[1][0], agents[1][1]) 
